{
  "expo": {
    "name": "Secopcol",
    "slug": "secopcol",
    "scheme": "secopcol",
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.secopcol.app",
      "icon": {
        "light": "./assets/secopcol-exports/dark-iOS-Default-1024x1024@1x.png",
        "dark": "./assets/secopcol-exports/dark-iOS-Default-1024x1024@1x.png",
        "tinted": "./assets/secopcol-exports/dark-iOS-TintedLight-1024x1024@1x.png"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Secopcol usa tu ubicaci贸n para mostrarte procesos de contrataci贸n cercanos a ti.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "monochromeImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.secopcol.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Secopcol usa tu ubicaci贸n para mostrarte procesos de contrataci贸n cercanos a ti."
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "color": "#007AFF",
          "defaultChannel": "secop-alerts",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-background-task",
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#000000",
          "barStyle": "light-content"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "65e7a02b-bd95-4f45-86b5-328373503f1a"
      }
    }
  }
}
